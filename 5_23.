//23Put even and odd elements of an array in two separate arrays.
#include<stdio.h>
void separate(int[],int);
	int main(){
	int i,n;
	printf("Enter the size of array:\n");
	scanf("%d",&n);
	int a[n];
	printf("Enter the array of element:\n");
	for(i=0;i<n;i++)
	{
	scanf("%d",&a[i]);
	}
	separate(a,n);
}
//Defination
void separate(int a[],int n);
